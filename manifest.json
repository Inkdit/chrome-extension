{
    "manifest_version": 2,
    "name": "Inkdit Notifications",
    "description": "Receive Inkdit notifications in your browser",
    "version": "0.2",
    "icons": {
        "128": "Inkdit-Icon-Extension.png"
    },
    "browser_action": {
				"default_title": "",
        "default_icon": "Inkdit-Icon-38.png",
				"default_popup": "popup.html"
    },
		"background": {
			"scripts": [
				"jquery.min.js",
				"background.js"
			]
		},
		"permissions": [
			"https://inkdit.com/"
		]
}
